<h1>會員登入</h1>

<%= form_for(@user,url: login_users_path) do |form| %>
<div class="fields">
<%= form.label :account, '帳號'%>
<%= form.text_field :account,placeholder: "請輸入帳號或email" %>
</div>
<div class="fields">
<%= form.label :password, '密碼' %>
<%= form.password_field :password %>
</div>
<div class="fields">
<%= form.submit "登入" %>
</div>
<% end %>